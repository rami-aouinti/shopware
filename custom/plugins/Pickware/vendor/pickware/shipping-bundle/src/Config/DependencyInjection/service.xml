<?xml version="1.0" ?>
<container
    xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="Pickware\ShippingBundle\Config\ConfigService">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
        </service>
        <service id="Pickware\ShippingBundle\Config\ContextResolver">
            <argument type="service" id="Pickware\DalBundle\EntityManager" />
        </service>
        <service id="Pickware\ShippingBundle\Config\ShipmentConfigurationService">
            <argument type="service" id="Pickware\ShippingBundle\Config\ContextResolver" />
        </service>
    </services>
</container>
